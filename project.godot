; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Newman Network: Channel 64"
config/description="3D Collectathon Platformer"
config/version="alphademo-release"
run/main_scene="res://scenes/root.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.0705882, 0.0509804, 0.0784314, 1)
boot_splash/show_image=false
boot_splash/image="res://cinder.png"
boot_splash/fullsize=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon_win.ico"

[autoload]

PlayerDataManager="*res://scenes/pdm.tscn"
AudioManager="*res://scripts/audio_manager.gd"
InputManager="*res://scripts/input_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
LUTShard="*res://scripts/lut_shard.gd"

[debug]

settings/crash_handler/message="Newman Network: Channel 64 has crashed! Please get in touch about this bug at the following address: askjefftastic@gmail.com. Be sure to include the following details."

[dialogue_manager]

general/wrap_lines=true
general/states=["AudioManager", "InputManager", "PlayerDataManager"]
general/new_with_template=false

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/initial_position_type=3
window/size/initial_position=Vector2i(215, 0)
window/size/resizable=false
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[gui]

theme/default_font_multichannel_signed_distance_field=true

[importer_defaults]

mp3={
"bar_beats": 4,
"beat_count": 0,
"bpm": 0,
"loop": true,
"loop_offset": 0
